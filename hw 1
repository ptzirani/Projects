#include<iostream> 
using namespace std;


int number=1;
while (number<=10000){
cout << number<<;
number=number*3;
}
int number=1;
while (number<=21){
cout << number<<;
number=number+2;
}

int number=3;
int sum=0;
while (number<=30){
cout << number<<;

number=number+3;
sum+=number;
}
cout<< sum<<;




}
#include<iostream> 
using namespace std;
int main(){

int number = 1;
while(number <= 10000) {
	cout << number ;
	number = number * 3;
}
int number = 1;
while(number <= 21) {
	cout << number ;
	number = number + 2;
}

int number = 3;
int sum = 0;
while(number <= 30) {
	cout << number ;

	number = number + 3;
	sum += number;
}
cout << sum ;

}
